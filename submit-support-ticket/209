<script>
  $(document).ready(function() {
    $( "#tfa_16" ).on('change', function() {

        var activeFilter = $("#tfa_16").val();
        console.log(activeFilter);
        $('#tfa_49').val('');
        $("#tfa_50").hide();
        $("#tfa_51").hide();
        $("#tfa_52").hide();
        $("#tfa_53").hide();
        var subCategory = null;
        switch(activeFilter) {
            case "tfa_17":
            subCategory = "#tfa_50";
            break;
            case "tfa_18":
            subCategory = "#tfa_51";
            break;
            case "tfa_19":
            subCategory = "#tfa_52";
            break;
            case "tfa_20":
            subCategory = "#tfa_53";
            break;
        }
        $(subCategory).prop("disabled", false).show();
    });
  });
</script>
  
